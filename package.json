{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "description": "Frontend Boilerplate",
  "license": "MIT",
  "private": true,
  "repository": {
    "url": "https://github.com/bungu/frontend-boilerplate"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "./node_modules/.bin/webpack --mode=production --config=config/webpack/client.prod.js --hideModules",
    "build:server": "./node_modules/.bin/webpack --mode=production --config=config/webpack/server.prod.js --hideModules",
    "build": "npm run build:client && npm run build:server",
    "dev:build:client": "./node_modules/.bin/webpack --mode=development --config=config/webpack/client.dev.js --hideModules",
    "dev:build:server": "./node_modules/.bin/webpack --mode=development --config=config/webpack/server.dev.js --hideModules",
    "dev:build": "npm run dev:build:client && npm run dev:build:server",
    "docker:build": "docker build . -t bungubot/frontend-boilerplate",
    "docker:run": "docker run --name frontend-boilerplate --rm -p 8080:8080 -it bungubot/frontend-boilerplate",
    "start": "node --icu-data-dir=node_modules/full-icu dist/server.js"
  },
  "devDependencies": {
    "@types/csso": "^3.5.0",
    "@types/express": "^4.16.0",
    "@types/morgan": "^1.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.0.11",
    "@types/react-loadable": "^5.4.1",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "morgan": "^1.9.1",
    "terser-webpack-plugin": "^2.3.5",
    "typescript": "^3.8.3",
    "webpack": "^4.25.1",
    "webpack-async-chunk-names-plugin": "^0.1.1",
    "webpack-cli": "^3.1.2",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "dependencies": {
    "@tutu/test-component": "0.0.1",
    "csso": "^4.0.2",
    "express": "^4.16.4",
    "express-static-gzip": "^2.0.6",
    "fast-memoize": "^2.5.1",
    "full-icu": "^1.3.1",
    "i18next": "^19.5.4",
    "i18next-browser-languagedetector": "^5.0.0",
    "i18next-http-backend": "^1.0.16",
    "jss": "^10.0.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-i18next": "^11.7.0",
    "react-jss": "^10.0.4",
    "react-loadable": "^5.5.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0"
  },
  "main": "dist/server.js",
  "directories": {
    "lib": "lib"
  },
  "author": "Artem Nechunaev <artem@nechunaev.com>",
  "sideEffects": false
}
