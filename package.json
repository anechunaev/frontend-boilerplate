{
  "name": "frontend-boilerplate",
  "version": "0.0.1",
  "description": "Frontend Boilerplate",
  "license": "MIT",
  "private": true,
  "repository": {
    "url": "https://github.com/bungu/frontend-boilerplate"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cross-env WEBPACK_CONFIG=client_prod,server webpack --colors --hide-modules",
    "build-client": "cross-env WEBPACK_CONFIG=client_prod webpack --colors --hide-modules",
    "build-server": "cross-env WEBPACK_CONFIG=server webpack --colors --hide-modules",
    "build-run": "cross-env WEBPACK_CONFIG=client_prod,server webpack --colors --hide-modules && node dist/server",
    "css-types": "tcm -p 'src/**/!(*.global).@(+(css|less))'",
    "lint": "eslint .",
    "clean": "rimraf dist",
    "dev-server": "cross-env WEBPACK_CONFIG=client_dev webpack-dev-server",
    "server": "node dist/server",
    "start": "npm run build && npm run server"
  },
